{
  "version": 2,
  "builds": [
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main.py"
    }
  ],
  "env": {
    "GOOGLE_MAPS_API_KEY": "@google_maps_api_key",
    "CORS_ORIGINS": "https://your-app.vercel.app"
  }
}
